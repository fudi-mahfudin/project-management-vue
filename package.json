{
  "name": "project-management-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "db:link": "supabase link --project-ref fxbysiaacmdpajwxaxoo",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked --yes",
    "db:seed": "node --env-file=.env.local src/database/seed.js",
    "db:types": "supabase gen types typescript --project-id fxbysiaacmdpajwxaxoo --schema public > src/database/types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.43.4",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "supabase": "^2.39.2",
    "typescript": "~5.8.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
